{
	"name": "jtojnar/entries",
	"description": "Entry registration system for rogaining",
	"homepage": "http://github.com/jtojnar/entries",
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Tojnar",
			"homepage": "http://ogion.cz"
		}
	],
	"repositories": [
	],
	"require": {
		"php": ">= 7.1",
		"nette/application": "~2.4",
		"nette/bootstrap": "~2.4",
		"nette/caching": "~2.5",
		"nette/database": "~2.4",
		"nette/di": "~2.4",
		"nette/finder": "~2.4",
		"nette/forms": "~2.4",
		"nette/http": "~2.4",
		"nette/mail": "~2.4",
		"nette/robot-loader": "~3.0",
		"nette/safe-stream": "~2.3",
		"nette/security": "~2.4",
		"nette/utils": "~2.4",
		"latte/latte": "~2.4",
		"tracy/tracy": "~2.4",
		"nextras/forms": "~2.0",
		"kdyby/forms-replicator": "dev-master",
		"kdyby/translation": "~2.4",
		"nextras/orm": "~3.0",
		"pelago/emogrifier": "^1.2"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.3",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"nette/tester": "^2.0.0",
		"phpstan/phpstan": "^0.9.0",
		"phpstan/phpstan-nette": "^0.9.0"
	},
	"autoload": {
		"classmap": ["app/"]
	},
	"scripts": {
		"cs": "vendor/bin/php-cs-fixer fix --verbose --dry-run --diff --config=utils/php_cs.php",
		"lint": "vendor/bin/parallel-lint app/ tests/ www/",
		"phpstan": "vendor/bin/phpstan analyse --configuration=utils/phpstan.neon --level=7 app/ tests/ www/"
	},
	"config": {
		"platform": {
			"php": "7.1.0"
		}
	}
}
