{
	"name": "jtojnar/entries",
	"description": "Entry registration system for rogaining",
	"homepage": "http://github.com/jtojnar/entries",
	"type": "project",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Tojnar",
			"homepage": "http://ogion.cz"
		}
	],
	"require": {
		"php": ">= 7.1",
		"ihor/nspl": "^1.3",
		"nette/application": "~3.0",
		"nette/bootstrap": "~3.0",
		"nette/caching": "~3.0",
		"nette/database": "~3.0",
		"nette/di": "~3.0",
		"nette/finder": "~2.5",
		"nette/forms": "^3.0",
		"nette/http": "~3.0",
		"nette/mail": "~3.0",
		"nette/safe-stream": "~2.4",
		"nette/security": "~3.0",
		"nette/utils": "~3.0",
		"latte/latte": "~2.5",
		"tracy/tracy": "~2.6",
		"nextras/form-components": "^1.0.0",
		"nextras/forms-rendering": "^1.0.0",
		"kdyby/forms-replicator": "^2.0.0",
		"contributte/translation": "^0.6.0",
		"nextras/orm": "~3.1",
		"pelago/emogrifier": "^3.1"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.3",
		"jakub-onderka/php-parallel-lint": "^1.0",
		"nette/tester": "^2.2.0",
		"nextras/mail-panel": "^2.5",
		"nextras/orm-phpstan": "dev-master#dbe387a",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-nette": "^0.12.0"
	},
	"autoload": {
		"classmap": ["app/"]
	},
	"scripts": {
		"cs": "vendor/bin/php-cs-fixer fix --verbose --dry-run --diff --config=utils/php_cs.php",
		"lint": "vendor/bin/parallel-lint app/ tests/ www/",
		"phpstan": "vendor/bin/phpstan analyse --configuration=utils/phpstan.neon"
	},
	"config": {
		"platform": {
			"php": "7.1.3"
		}
	}
}
