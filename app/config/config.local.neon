			XJ:
				constraints:
					- 'some(gender=male)'
					- 'some(gender=female)'
					- 'all(age<=18)'
			MO:
				constraints:
					- 'all(gender=male)'
			WO:
				constraints:
					- 'all(gender=female)'
			XO:
				constraints:
					- 'some(gender=male)'
					- 'some(gender=female)'
			MV:
				constraints:
					- 'all(gender=male)'
					- 'all(age>=40)'
			WV:
				constraints:
					- 'all(gender=female)'
					- 'all(age>=40)'
			XV:
				constraints:
					- 'some(gender=male)'
