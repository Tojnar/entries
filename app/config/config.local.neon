php:
	date.timezone: Europe/Prague

parameters:
	siteTitle:
		cs: 7. Sudetské Tojnárkovo Bloudění 2021
		en: Lost in Tojnarek’s Sudetenland 2021
	entries:
		maxMembers: 2
		minMembers: 2
		eventDate: 2021-09-24 # used for age calculations
		opening: 2021-06-21T00:00:00
		closing: 2021-09-24T14:00:00
		invoiceModifier: App\InvoiceModifier::modify
		fees:
			person: 0
